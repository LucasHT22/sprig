---
import Categories from '../../../layouts/categories.astro'
import Translations from '../translations/translations.json'

const response = await fetch(
    `https://${Astro.url.host}/translations/${
        Astro.url.searchParams.get("lang") || Translations[0]
    }.json`
);
console.log(response);
const data = await response.json();
---
<Categories name={data["tutorials.categories.name"]} title={data["tutorials.categories.title"]} whats={data["tutorials.categories.whats"]} about={data["translations.categories.about"]} />