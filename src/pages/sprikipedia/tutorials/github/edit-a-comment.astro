---
import '../../../../global.css'
import Docs from '../../../../layouts/docs.astro'
import Translations from '../../../../pages/sprikipedia/translations/translations.json'

const response = await fetch(
    `https://${Astro.url.host}/translations/${
        Astro.url.searchParams.get("lang") || Translations[0]
    }.json`
);
console.log(response);
const data = await response.json();
---

<Docs dspriki={data["tutorials.categories.dspriki.github.comment"]} dtitle={data["tutorials.categories.dtitle.github.comment"]} dauthor={data["tutorials.categories.dauthor.github.comment"]} >

</Docs>